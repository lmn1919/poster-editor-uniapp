.drag-right,.drag-left{
  height: 14px;
  width: 6px;
  background: red;
  border-radius: 5rpx;
  display: inline-block;
  position: absolute;
}
.drag-right{
	right: -4px;
}
.drag-left{
	left: -4px;
}
.drag-top,.drag-bottom{
  width: 14px;
  height: 6px;
  background: red;
  border-radius: 5rpx;
  display: inline-block;
  position: absolute;
}
.drag-top{
  top:-4px;
}
.drag-bottom{
	bottom: -4px;
}


